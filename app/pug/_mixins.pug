mixin search
    .search(style="background-image: url(img/search-bg.jpg)")
            .container
                .search__inner
                    .search__inner-title Welcome To Foxtar Market Place!
                    .search__inner-subtitle Premium WordPress Themes, Web Templates and Many More ...
                    .search__inner-form
                        form
                            input(type="text" placeholder="Search Your Keywords . . ." required)
                            button(type="submit")
                                img(src="img/icon/search.png", alt="")

